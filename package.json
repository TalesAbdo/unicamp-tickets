{
  "name": "unicamp-tickets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "concurrently \"vue-cli-service serve\" \"nodemon server/server.js\"",
    "main": "vue-cli-service serve"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bulma": "^0.7.4",
    "canvas-to-blob": "^1.0.0",
    "chart.js": "^2.8.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "file-saver": "^2.0.2",
    "moment": "^2.24.0",
    "mysql2": "^1.6.5",
    "nodemon": "^1.19.1",
    "sequelize": "^5.8.5",
    "swagger-jsdoc": "^1.3.0",
    "swagger-ui-express": "^4.0.7",
    "uuid": "^3.3.3",
    "vue": "^2.6.6",
    "vue-awesome": "^3.5.1",
    "vue-cli-plugin-resolve-alias": "^0.1.3",
    "vue-notification": "^1.3.16",
    "vue-router": "^3.0.1",
    "vuelidate": "^0.7.4",
    "vuex": "^3.0.1",
    "xlsx": "^0.14.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-async-to-generator": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "import/extensions": [
        "off"
      ],
      "max-len": [
        "error",
        200
      ],
      "global-require": 0,
      "linebreak-style": 0,
      "no-param-reassign": 0,
      "no-console": 0,
      "no-throw-literal": 0,
      "import/no-dynamic-require": 0,
      "indent": [
        "error",
        4
      ],
      "comma-dangle": [
        "error",
        "only-multiline"
      ],
      "import/no-unresolved": [
        "off"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 0.5%",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
